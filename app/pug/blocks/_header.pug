							.cart__total-text Total
							.cart__total-summ $500
						button.btn.cart__btn Checkout
						a.btn.cart__link(href="#") Go to cart
				details.user
					summary.user__summary My Account
						i.icon-caret-down
					ul.user__dropdown
						li.user__item
							a.user__link(href='#') Login / Register
						li.user__item
							a.user__link(href='#') Support
